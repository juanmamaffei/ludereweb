<p id="notice"><%= notice %></p>

<h1>Materia</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Nivel</th>
      <th>Descripcion</th>
      <th>Objetivos</th>
      <th>Bibliografia</th>
      <th>Evaluacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @materia.each do |materium| %>
      <tr>
        <td><%= materium.nombre %></td>
        <td><%= materium.nivel %></td>
        <td><%= materium.descripcion %></td>
        <td><%= materium.objetivos %></td>
        <td><%= materium.bibliografia %></td>
        <td><%= materium.evaluacion %></td>
        <td><%= link_to 'Show', materium %></td>
        <td><%= link_to 'Edit', edit_materium_path(materium) %></td>
        <td><%= link_to 'Destroy', materium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Materium', new_materium_path %>
